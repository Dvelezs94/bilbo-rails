$("#paginator a").hide();
$("#boardElements").append('#{escape_javascript render(partial: "shared/loading_ads")}');
setTimeout(function() {
$('#spinner').remove();
$('#boardElements').append('#{escape_javascript render(partial: "boards/provider/board_row", collection: @boards, as: :board)}');
setImageLoader();
$('#paginator').html('#{escape_javascript(link_to_next_page @boards, "#{t("bilbos.more_bilbos")}"'<i class="ion-arrow-down-c" style="padding-left:5px;"></i>'.html_safe, :remote => true, :param_name => "upcoming_page")}');
$("#paginator a").show();
initLightbox();
}, 1000);
